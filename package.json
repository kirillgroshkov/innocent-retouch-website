{
  "name": "innocent-retouch-website",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "clean-www": "rm -rf ./www",
    "tslint-all": "tslint './src/**/*.{ts,tsx}' -e './src/components.d.ts' -t stylish --fix",
    "prettier-do": "prettier --write '**/*.{js,jsx,ts,tsx,css,scss,graphql,json}' '*.{md,js}'",
    "prettier-all": "yarn prettier-do && yarn tslint-all"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "tslint -e '**/components.d.ts' -t stylish --fix",
      "git add"
    ],
    "src/**/*.{css,scss,graphql,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@stencil/core": "^0.4.3",
    "@stencil/router": "^0.0.25",
    "bootstrap": "^4.0.0",
    "fg-loadcss": "^2.0.1",
    "jquery": "^3.2.1",
    "popper.js": "1.12.9"
  },
  "devDependencies": {
    "@stencil/dev-server": "^0.0.18",
    "@stencil/utils": "latest",
    "@types/bluebird": "^3.5.18",
    "@types/fs-extra": "^5.0.0",
    "@types/klaw-sync": "^1.1.3",
    "@types/sharp": "^0.17.5",
    "bluebird": "^3.5.1",
    "cloudinary": "^1.9.1",
    "dotenv": "^5.0.0",
    "fs-extra": "^5.0.0",
    "husky": "^0.14.3",
    "klaw-sync": "^3.0.2",
    "lint-staged": "^6.1.0",
    "prettier": "^1.8.2",
    "sharp": "^0.19.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.0"
  },
  "author": "Kirill Groshkov <1@inventix.ru>",
  "license": "MIT"
}
